<body class="register-page full-screen sidebar-collapse" data-gr-c-s-loaded="true">
  <div class="wrapper">
    <div class="page-header" style="background-image: url('https://images.pexels.com/photos/298660/pexels-photo-298660.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260');">
      <div class="filter"></div>
      <div  class="container">
        <div  class="row">
          <div style='margin-left:35%'  class="col-md mr-auto">
            <div class="card card-register">
              <h3 class="card-title text-center" style='font-size:30px;font-family:"Comic Sans MS",cursive,sans-serif;'>Bookrest</h3>
              <p class='text-center' style="font-family:'Comic Sans MS',cursive,sans-serif;font-size:12px;color:#66615b">Find the hidden</p>
              <div class="social">
                <h4 style="color:#66615b">Enter new password</h4>
              </div>
<br>
              <form class="register-form" id='resetform' method='post'>
                <input type="password" class="form-control" id='password' name='password' placeholder="Password">
                <input type="password" class="form-control" id='password1' name='password1' placeholder="Confirm Password">

                <p id='msgs' style='color:red'></p>
                <button id='next' class="btn btn-block btn-round" type=submit >Change</button>
              </form>
              <div class="login">
                <p>Doesn't have an account?
                  <a href="/users/signup">Signup</a>.</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!--   Core JS Files   -->
  <script src="https://demos.creative-tim.com/paper-kit-2-pro/assets/js/core/jquery.min.js" type="text/javascript"></script>
